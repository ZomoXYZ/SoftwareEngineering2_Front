; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="WAN"
run/main_scene="res://scenes/main_menu/Main_Menu.tscn"
config/features=PackedStringArray("4.0")
config/icon="res://icon.png"

[autoload]

StartVars="*res://Start_Vars.gd"
Request="*res://HTTPAuth/request.gd"
UserData="*res://HTTPAuth/userdata.gd"
LobbyConn="*res://HTTPAuth/lobby.gd"
RequestEnv="*res://HTTPAuth/request_env.gd"

[display]

window/size/viewport_width=1080
window/size/viewport_height=1920
window/handheld/orientation=1
window/size/width=480
window/size/height=853

[editor]

version_control_autoload_on_startup=true
version_control_plugin_name="GitAPI"

[gdnative]

singletons=[]

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

2d/default_gravity=0
2d/default_gravity_vector=Vector2(0, 0)
common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
